{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/.*",
            "dest": "./routes/transactions.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}

  